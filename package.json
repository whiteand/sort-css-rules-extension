{
  "name": "sort-css-rules",
  "displayName": "sort-css-rules",
  "description": "Sorts css rules within selection",
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "sort-css-rules.sort",
        "title": "Sort CSS Rules: Sort Selected CSS Properties"
      },
      {
        "command": "sort-css-rules.sort-all",
        "title": "Sort CSS Rules: Sort All CSS Properties in the File"
      }
    ]
  },
  "publisher": "AndrewBeletskiy",
  "repository": {
    "type": "git",
    "url": "https://github.com/whiteand/sort-css-rules-extension.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/scss-parser": "^1.0.1",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "scss-parser": "^1.0.6"
  }
}
